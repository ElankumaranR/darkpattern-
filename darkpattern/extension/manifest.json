{
  "manifest_version": 3,
  "name": "Dark_Pattern",
  "version": "1.0",
  "description": "Alert the user from dark patterns",
  "permissions": [
    "activeTab",
    "tabs",
    "scripting",
    "webRequest"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon16.png"
  },
  "background": {
    "service_worker": "background.js"
  },

  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }

}
